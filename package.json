{
  "name": "nest-watcha",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev-backup": "nest start --watch",
    "start:local": "cross-env NODE_ENV=local nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:dev": "cross-env NODE_ENV=dev nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:prod": "cross-env NODE_ENV=prod pm2 start dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop",
    "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync",
    "db:migrate": "npm run typeorm migration:run",
    "db:migrate:revert": "npm run typeorm migration:revert",
    "db:create-migration": "npm run typeorm migration:create -- -n",
    "db:generate-migration": "npm run typeorm migration:generate -- -n"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.6.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.2.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/elasticsearch": "^9.0.0",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/passport": "^8.2.2",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^1.1.0",
    "@nestjs/swagger": "^5.0.9",
    "@nestjs/typeorm": "^8.1.4",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/passport-kakao": "^0.2.1",
    "aws-sdk": "^2.999.0",
    "axios": "^0.26.1",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^4.1.0",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.8",
    "dotenv": "^10.0.0",
    "exception": "^1.0.9",
    "express-session": "^1.17.2",
    "joi": "^17.6.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "multer": "^1.4.3",
    "multer-s3": "^2.9.0",
    "mysql2": "^2.3.3",
    "openai": "^3.1.0",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-kakao": "^1.0.1",
    "passport-local": "^1.0.0",
    "passport-naver": "^1.0.6",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "solr-node": "^1.2.1",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.3.10"
